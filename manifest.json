
 {
   "manifest_version": 2,

   "name": "Text analytics",
   "description": "Analyze Text",
   "version": "1.0",

   "permissions": [
     "tabs", "http://*/*", "https://*/*", "*://*/*"
   ],
   "browser_action": {
   },

     "background": {
     "scripts": ["jquery-3.1.1.min.js", "background.js", "apiKey.js"],
     "persistent": false
   },
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": ["application.js"]
     }
   ]

 }
